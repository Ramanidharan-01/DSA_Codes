'''
# Node Class    
class Node:
    def __init__(self, val):
        self.data = val
        self.next = None
'''
class Solution:
    def areIdentical(self, head1, head2):
        while head1 or head2:
            if not(head1) or not(head2):
                return False
            if head1.data != head2.data:
                return False
            head1=head1.next
            head2=head2.next
        return True
