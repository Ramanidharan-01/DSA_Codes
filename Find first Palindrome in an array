class Solution:
    def palindrome (self,s):
        i,j=0,len(s)-1
        while(i<j):
            if s[i]!=s[j]:
                return False
            i+=1
            j-=1
        return True
    def firstPalindrome(self, words: List[str]) -> str:
        for word in words:
            if self.palindrome(word):
                return word
        return ""
        
