# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def mergeTwoLists(self, list1, list2):
        dummy = ListNode(-1)
        temp=dummy
        temp1=list1
        temp2=list2
        while temp1 or temp2:
            if not(temp1):
                temp.next=temp2
                break
            elif not(temp2):
                temp.next=temp1
                break
            elif temp1.val<=temp2.val:
                temp.next=temp1
                temp1=temp1.next
            else:
                temp.next=temp2
                temp2=temp2.next
            temp=temp.next
        return dummy.next
